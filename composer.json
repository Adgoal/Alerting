{
    "name": "adgoal-common/alerting",
    "type": "library",
    "description": "Adgoal Alerting common library",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-redis": "*",
        "adgoal-common/base": "^4.0",
        "adgoal-common/value-object": "v3.9.5",
        "beberlei/assert": "^3.2",
        "queue-interop/amqp-interop": "^0.8",
        "queue-interop/queue-interop": "^0.7|^0.8",
        "enqueue/enqueue": "^0.10",
        "league/tactician-command-events": "^0.6.0",
        "psr/log": "^1.1",
        "php-http/guzzle6-adapter": "^2.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "jakub-onderka/php-console-highlighter": "^0.4",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "mockery/mockery": "^1.3",
        "phpmd/phpmd": "^2.9",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-mockery": "^0.11.0",
        "phpstan/phpstan-phpunit": "^0.11",
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master",
        "sensiolabs-de/deptrac-shim": "^0.5",
        "symfony/phpunit-bridge": "*",
        "symplify/easy-coding-standard": "^6.0",
        "vimeo/psalm": "3.15.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AdgoalCommon\\Alerting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdgoalCommon\\Alerting\\Tests\\Unit\\": "tests/unit"
        }
    },
    "repositories": [
        {
            "packagist.org": false
        },
        {
            "type": "composer",
            "url": "https://repo.packagist.org/"
        },
        {
            "type": "composer",
            "url": "https://satis.adgoaltech.net"
        }
    ]
}
